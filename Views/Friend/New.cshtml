@model Friend
<h1>Add New Friend</h1>

<form action="@Url.Action("Index","Friend")" method="post">
    <div id="new_friend_form">
        <input type="text" name="FriendName" id="FriendNameInput" placeholder="Friend Name">
        <input type="text" name="Place" id="PlaceInput" placeholder="Place">

        <button>Add Friend</button>
    </div>
</form>